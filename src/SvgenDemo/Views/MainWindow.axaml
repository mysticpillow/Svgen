<Window
    x:Class="SvgenDemo.Views.MainWindow"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:resources="clr-namespace:SvgenDemo.Resources;assembly=SvgenDemo.Resources"
    xmlns:vm="using:SvgenDemo.ViewModels"
    Title="SvgenDemo"
    d:DesignHeight="450"
    d:DesignWidth="800"
    x:DataType="vm:MainWindowViewModel"
    mc:Ignorable="d">
    <DockPanel>
        <ContentControl
            Width="200"
            Height="200"
            Background="AntiqueWhite">
            <Image
                HorizontalAlignment="Stretch"
                VerticalAlignment="Stretch"
                Source="{x:Static resources:SvgResources.A}" />
        </ContentControl>
    </DockPanel>
</Window>